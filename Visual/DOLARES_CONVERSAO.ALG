Algoritmo "dolares_conversao"
// 

Var
cont,r,re,conv:real

Inicio

escreval("Quantas conversões será feita: ")
leia(conv)

cont <-0
enquanto (cont<conv)faca

escreval("Digite uma valor real para a conversão em dolar: ")

leia(r)
cont<-cont+1

re <- r/5,12
escreval("O resultado da conversão é: ",re)

fimenquanto

Fimalgoritmo