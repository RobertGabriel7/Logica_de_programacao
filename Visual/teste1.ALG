Algoritmo "valor_e_total"
//  
//  



Var

valor,cont,total,maior:inteiro



Inicio

cont <- 0
total <- 0
enquanto (cont<5)faca
escreval("Digite um valor: ")
leia(valor)

se (valor>maior)entao
maior<- valor
fimse
total <- total + valor
cont <- cont + 1
fimenquanto

escreval("O valor total é: ",total)
escreval("O maior valor: ",maior)
Fimalgoritmo