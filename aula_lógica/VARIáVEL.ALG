algoritmo "Variável"
   Var
    idade : inteiro
    altura : real
    nome : caractere
    casado : logico
    vet_uni : vetor [0..4] de real
    vet_bi : vetor [0..2, 0..2] de inteiro
inicio // seção de comandos
    idade <- 19
    altura <- 1.75
    nome <- "Robert Gabriel"
    casado <- falso
    vet_uni [4] <- 77
    vet_bi [1,1] <- 777
fimalgoritmo